//LL-array of nodes
/******************************************************************************
#include <iostream>

using namespace std;
struct node
{
  int data;
  struct node *next;
}n[10];

int main()
{
    for(int i=0;i<10;i++)
{ n[i].data=i;
    n[i].next=&n[i+1];
}
for(int i=0;i<10;i++)
{
 cout<<n[i].data;
}
    return 0;
}

OUTPUT:
0123456789
